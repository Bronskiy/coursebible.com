{
  "require": {
    "carlcs/craft-redactorcustomstyles": "3.0.2",
    "codemonauts/craft-instagram-feed": "1.0.4",
    "craftcms/cms": "3.3.18.1",
    "craftcms/contact-form": "2.2.5",
    "craftcms/contact-form-honeypot": "1.0.2",
    "craftcms/element-api": "2.6.0",
    "craftcms/redactor": "2.4.0",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
